{
    "manifest_version": 2,
    "name": "Dev Assistant",
    "short_name": "Dev Assist",
    "version": "0.1",
    "description": "Help to manage requests.",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icon_128.png"
        },
        "default_title": "Dev Assistant",
        "default_popup": "menu.html"
    },
    "background": {
        "scripts": [
            "static/js/runtime-background.js",
            "static/js/0.js",
            "static/js/background.js"
        ],
        "persistent": true
    },
    "commands": {
        "open-ja-dashboard": {
            "suggested_key": {
                "default": "Alt+D",
                "mac": "Command+Shift+D"
            },
            "description": "Open Dev Assistant dashboard page"
        }
    },
    "content_security_policy": "script-src 'self' ; object-src 'self';",
    "offline_enabled": true,
    "permissions": [
        "storage",
        "unlimitedStorage",
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "proxy",
        "http://*/",
        "https://*/"
    ],
    "optional_permissions": [],
    "author": "Shridhar"
}